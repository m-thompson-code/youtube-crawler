{
  "name": "moo-youtube-crawler",
  "version": "0.2.0",
  "description": "Do some youtube magic",
  "main": "bundle.js",
  "scripts": {
    "test": "npx jest",
    "watch": "nodemon -e ts --watch lib --exec \"npm run build\"",
    "build": "rimraf out-tsc && tsc && rollup out-tsc/index.js --file bundle.js --format iife",
    "prepublish": "npm run test && npm run build",
    "start": "npm run build && npm run serve",
    "serve": "http-server -S",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-thompson-code/youtube-crawler.git"
  },
  "keywords": [
    "youtube",
    "crawler"
  ],
  "author": "Mark Thompson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/m-thompson-code/youtube-crawler/issues"
  },
  "homepage": "https://github.com/m-thompson-code/youtube-crawler#readme",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.4.6",
    "http-server": "^14.1.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "typescript": "^4.5.5"
  }
}
